<Page x:Class="AutoserviceApp.AutoPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AutoserviceApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AutoPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Content="Введите логин"></Label>
        <TextBox Grid.Row="1" Name="Login"></TextBox>
        <Label Grid.Row="2" Content="Введите пароль"></Label>
        <TextBox Grid.Row="3" Name="Password"></TextBox>
        <Button Grid.Row="4" x:Name="Reg" Click="Reg_Click" Content="Регистрация" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5"></Button>
        <Button Grid.Row="5" x:Name="Enter" Click="Enter_Click" Content="Войти" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5"></Button>
    </Grid>
</Page>
